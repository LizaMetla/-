#include <iostream>
#include <stdlib.h>
#include <locale>


void entermass(int n, int mass[])
{
	int i;
	for (i = 0; i < n; i++) {
		printf("mass[%d] = ", i);
		scanf_s("%d", &mass[i]);
	}

}


void output_mass(int n, int mass[]) {


	for (int m = n - 1; m >= 0; m--) 
	{
		printf("%d ", mass[m] * 3);
	}
	printf("\n");
}

int main()
{
	setlocale(LC_ALL, "Russian");
	int n, s, j, k;
	
	printf("Введите n: ");
	scanf_s("%d", &n);


	int *m_n = new int[n];

	entermass(n, m_n);


	output_mass(n, m_n);
	return 0;

}


